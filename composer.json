{
    "name": "magento/magento-semver",
    "description": "Magento Semantic Version Checker",
    "version": "11.0.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "bin": ["bin/svc"],
    "require": {
        "php": "~7.4.0||~8.1.0",
        "ext-json": "*",
        "laminas/laminas-dependency-plugin": "^2.2.0",
        "laminas/laminas-stdlib": "~3.6.0",
        "nikic/php-parser": "~4.12.0",
        "phpstan/phpdoc-parser": "^0.5.5",
        "sabre/xml": "~2.2.3",
        "symfony/console": "~4.4.0||~5.4.0",
        "tomzx/php-semver-checker": "^0.15.0",
        "wikimedia/less.php": "~3.1.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "~9.5.8",
        "squizlabs/php_codesniffer": "~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Magento\\SemanticVersionChecker\\": "src/",
            "Magento\\SemanticVersionChecker\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
