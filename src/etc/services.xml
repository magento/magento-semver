<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="console.command.ids_adsf" type="collection">
            <parameter>mail1</parameter>
            <parameter>mail2</parameter>
            <parameter>mail3</parameter>
        </parameter>
    </parameters>

    <services>
        <service id="backwardincompatiblechangescommand"
                 class="\Magento\SemanticVersionChecker\Console\Command\BackwardIncompatibleChangesCommand">
            <tag name="console.command"/>
        </service>
        <service id="comparesourcecommand" class="\Magento\SemanticVersionChecker\Console\Command\CompareSourceCommand">
            <tag name="console.command"/>
        </service>
    </services>
</container>